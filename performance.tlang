
def fib(n) {
    var a = 0.0;
    var b = 1.0;
    var i = 0;
    while (i < n) {
        var c = a + b;
        a = b;
        b = c;
        i = i + 1;
    }
    return a;
}

def iter(msg) {
    var count = 0;
    for (char in msg) {
        count = count + 1;
    }
    return count;
}

// fib(10000000);

print(iter("Guys > Girls"));

